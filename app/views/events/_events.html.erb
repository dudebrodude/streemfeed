
   <div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Upcoming Events</h3>
  </div>
  <div class="panel-body">
       <table>
        <thead>
          <tr>
            <th>Date</th>
            <th>Event</th>
          </tr>
        </thead>
        <tbody>
          <% @club.events.each do |event| %>
          <tr>
            <td><%= event.date %></td>
            <td><%= event.name %></td>
            <td><%= event.club_id %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
  </div>
</div>



